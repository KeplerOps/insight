{
    "name": "@modelcontextprotocol/insight",
    "version": "1.0.0",
    "description": "An MCP server that enhances Cline with software development process automation, artifact management, and quality gates.",
    "type": "module",
    "main": "build/index.js",
    "bin": {
        "insight-mcp": "./build/index.js"
    },
    "scripts": {
        "build": "tsc && chmod +x build/index.js",
        "start": "node build/index.js",
        "dev": "tsc -w",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
        "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch"
    },
    "keywords": [
        "mcp",
        "cline",
        "development",
        "process",
        "automation"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.7",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.3"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.1.1"
    },
    "exports": {
        "@modelcontextprotocol/sdk": {
            "types": "./@modelcontextprotocol/sdk/types/index.d.ts",
            "default": "./@modelcontextprotocol/sdk/index.js"
        }
    }
}
